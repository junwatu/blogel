{
  "name": "blogel",
  "version": "0.0.1-alpha",
  "description": "Static page generator with editor",
  "main": "blogel.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/junwatu/blogel.git"
  },
  "scripts": {
    "start": "node blogel.js",
    "build": "npm run copy && npm run build-css && npm run-build-js",
    "build-js": "uglifyjs public/js/blogelweb.js -o public/js/blogelweb.min.js",
    "build-css": "sass scss/blogel.scss public/css/blogel.css",
    "watch-css": "onchange scss/blogel.scss -- npm run build-css",
    "copy": "scripts/copy-assets.sh"
  },
  "keywords": [
    "static",
    "page",
    "static page",
    "generator",
    "static page generator",
    "static page editor"
  ],
  "author": "Equan Pr.",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.0.2",
    "commander": "~2.1.0",
    "connect-flash": "~0.1.1",
    "consolidate": "~0.10.0",
    "cookie-parser": "^1.0.1",
    "express": "~4.1.2",
    "express-session": "^1.0.4",
    "github": "~0.1.12",
    "handlebars": "~1.3.0",
    "medium-editor": "^5.4.0",
    "method-override": "^1.0.0",
    "mkdirp": "^0.5.1",
    "moment": "~2.5.1",
    "nconf": "~0.6.9",
    "nedb": "~0.10.0",
    "passport": "~0.2.0",
    "passport-local": "~0.1.6",
    "uglify-js": "^2.4.23",
    "winston": "~0.7.2"
  },
  "devDependencies": {
    "node-sass": "^3.2.0",
    "onchange": "^1.1.0"
  }
}
